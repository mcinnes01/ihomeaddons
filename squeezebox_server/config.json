{
    "name": "Squeezebox Server",
    "version": "0.0.32",
    "slug": "squeezebox-server",
    "description": "Logitech Squeezebox Server Hass.IO addon",
    "url": "https://github.com/mcinnes01/ihomeaddons/squeezebox_server",
    "webui": "[PROTO:ssl]://[HOST]:[PORT:9000]",
    "ingress": true,
    "ingress_port": 0,
    "startup": "services",
    "panel_icon": "mdi:music",
    "arch": [
      "aarch64",
      "amd64",
      "armv7",
      "i386"
    ],
    "map": [
        "config:rw",
        "share:rw",
        "ssl"
    ],
    "boot": "auto",
    "hassio_api": true,
    "auth_api": true,
    "hassio_role": "default",
    "homeassistant_api": false,
    "options": {
      "ssl": true,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem",
      "media_path": "/share/media/music"
    },
    "schema": {
      "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)?",
      "ssl": "bool",
      "certfile": "str",
      "keyfile": "str",
      "media_path": "str",
      "leave_front_door_open": "bool?"
    },
    "ports": {
        "9000/tcp": 9000,
        "9090/tcp": 9090,
        "3483/tcp": 3483,
        "3483/udp": 3483
    }
}
