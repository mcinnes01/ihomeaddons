{
    "name": "Logitech Squeezebox Server",
    "version": "0.0.20",
    "slug": "squeezebox-server",
    "description": "Logitech Squeezebox Server Hass.IO addon",
    "url": "https://github.com/mcinnes01/ihomeaddons",
    "startup": "services",
    "arch": [
      "aarch64",
      "amd64",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "hassio_api": true,
    "hassio_role": "default",
    "homeassistant_api": false,
    "host_network": true,
    "map": [
        "config:rw",
        "share:rw",
        "ssl"
    ],
    "options": {
        "media_path": "/share/media/music"
    },
    "schema": {
        "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)?",
        "media_path": "str"
    },
    "ports": {
        "9000/tcp": 9000,
        "9090/tcp": 9090,
        "3483/tcp": 3483,
        "3483/udp": 3483
    }
}
